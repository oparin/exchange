            shadow: 0 //0 = no Shadow, 1,2,3 = 3 Different Art of Shadows -  (No Shadow in Fullwidth Version !)
        });
    });
    //]]
</script>
<!-- Revolution slider scripts ends -->

<!--other jqueries required-->
<script src="{{ asset('lib/megabiz/js/bootstrap.js') }}" type="text/javascript" ></script>
<script src="{{ asset('lib/megabiz/js/jquery.flexslider-min.js') }}" type="text/javascript" ></script>
<script src="{{ asset('lib/megabiz/js/jquery.prettyPhoto.js') }}" type="text/javascript"></script>
<script src="{{ asset('lib/megabiz/js/twitter.js') }}" type="text/javascript"></script>
